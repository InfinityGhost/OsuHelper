    <releaseNotes>https://github.com/Tyrrrz/OsuHelper/blob/master/Changelog.md</releaseNotes>
    <iconUrl>https://raw.githubusercontent.com/Tyrrrz/OsuHelper/master/favicon.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/Tyrrrz/OsuHelper/master/License.txt</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <bugTrackerUrl>https://github.com/Tyrrrz/OsuHelper/issues</bugTrackerUrl>
  </metadata>
  <files>
